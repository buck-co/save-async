{
  "name": "co.buck.datamanagement",
  "version": "0.2.0",
  "displayName": "BUCK Data Management",
  "description": "This is BUCK's Unity package for saving and loading data. It includes a DataManager class with an API for reading and writing data to disk.",
  "unity": "2023.2",
  "documentationUrl": "https://github.com/buck-co/unity-pkg-data-datamanagement.git",
  "changelogUrl": "https://github.com/buck-co/unity-pkg-data-datamanagement.git",
  "licensesUrl": "https://github.com/buck-co/unity-pkg-data-datamanagement.git",
  "keywords": [
    "BUCK",
    "Save",
    "Load",
    "Files",
    "I/O",
    "Data",
    "DataManager",
    "DataManagement"
  ],
  "repository": {
    "url": "https://github.com/buck-co/unity-pkg-data-datamanagement.git",
    "type": "git"
  },
  "author": {
    "name": "BUCK",
    "url": "https://buck.co/"
  },
  "dependencies": {
    "com.unity.nuget.newtonsoft-json": "3.2.1",
    "co.buck.basics": "https://github.com/buck-co/unity-pkg-buck-basics.git#v1.0.8",
    "jillejr.newtonsoft.json-for-unity.converters": "https://github.com/jilleJr/Newtonsoft.Json-for-Unity.Converters.git#v1.5.1"
  },
  "enableLockFile": true,
  "resolutionStrategy": "highest",
  "samples": [
      {
          "displayName": "Data Management Example",
          "description": "Contains a simple example scene that demonstrates the use of the DataManager class and two implementations of the ISaveable interface.",
          "path": "Samples~/DataManagementExample"
      }
  ]
}